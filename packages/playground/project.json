{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/playground/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/playground/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/playground"
      ],
      "options": {
        "jestConfig": "packages/playground/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/playground",
        "tsConfig": "packages/playground/tsconfig.lib.json",
        "packageJson": "packages/playground/package.json",
        "main": "packages/playground/src/index.ts",
        "assets": [
          "packages/playground/*.md"
        ]
      }
    },
    "watch": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "command": "tsc -w --outDir packages/playground/dist && cp package.json engrafia-plugin"
      }
    }
  },
  "tags": []
}